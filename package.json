{
  "type": "module",
  "name": "mon-inclusion-numerique",
  "version": "0.1.0",
  "license": "AGPL-3.0",
  "scripts": {
    "bash:production": "scalingo -a mon-inclusion-numerique run --size XL bash",
    "build": "./build.sh",
    "build:dev": "yarn prisma:migrate && yarn prisma:generate:fne && next build",
    "check": "yarn dedupe --check && yarn deadcode && yarn typecheck && yarn prisma format --check && yarn prisma format --check --schema seeds/fne/schema.prisma && yarn format:check && yarn lint:css && yarn lint:ts && yarn test:coverage",
    "db:start": "docker compose down && docker compose up -d && bash -c \"while ! docker compose logs | grep -q 'listening on IPv4 address'; do echo 'Waiting for database'; sleep 0.5; done\";",
    "deadcode": "knip",
    "depcheck": "depcheck",
    "dev": "yarn prisma:migrate && yarn prisma:generate:fne && next dev --turbo",
    "dotenv:test": "dotenv -e .env.test",
    "format": "prettier -w . \"!**/*.{js,jsx,mjs,mts,ts,tsx}\"",
    "format:check": "prettier -c . \"!**/*.{js,jsx,mjs,mts,ts,tsx}\"",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:ts": "eslint src seeds --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
    "migration:reset": "yarn prisma:reset && yarn migration:all",
    "migration:all": "yarn migration:region && yarn migration:departement && yarn migration:groupement && yarn migration:structure && yarn migration:utilisateur && yarn migration:gouvernance",
    "migration:departement": "tsx seeds/departement.ts",
    "migration:gouvernance": "tsx seeds/gouvernance.ts",
    "migration:groupement": "tsx seeds/groupement.ts",
    "migration:region": "tsx seeds/region.ts",
    "migration:structure": "tsx seeds/structure.ts",
    "migration:utilisateur": "tsx seeds/utilisateur.ts",
    "postinstall": "husky || true && ./post-install.sh",
    "prisma:generate:fne": "prisma generate --schema seeds/fne/schema.prisma",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset -f && yarn prisma:generate:fne",
    "psql:local": "docker compose up -d postgres-dev && docker exec -it min_db_dev psql -U min min",
    "psql:init": "bash scripts/psql-init.sh",
    "psql:test": "docker compose up -d postgres-test && docker exec -it min_db_test psql -U min min",
    "start": "next start",
    "test": "yarn dotenv:test -- yarn prisma:migrate && yarn dotenv:test -- vitest run",
    "test:coverage": "yarn test --coverage",
    "test:mutation:frontend": "yarn dotenv:test -- stryker run stryker-frontend.conf.json",
    "test:mutation:backend": "yarn dotenv:test -- stryker run stryker-backend.conf.json",
    "test:watch": "yarn dotenv:test -- vitest",
    "typecheck": "tsc",
    "bundle:analyze": "ANALYZE=true yarn build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.803.0",
    "@gouvfr/dsfr": "^1.13.1",
    "@next/bundle-analyzer": "^15.3.1",
    "@prisma/client": "^6.7.0",
    "@sentry/nextjs": "^9",
    "@tiptap/core": "^2.12.0",
    "@tiptap/extension-link": "^2.12.0",
    "@tiptap/pm": "^2.12.0",
    "@tiptap/react": "^2.12.0",
    "@tiptap/starter-kit": "^2.12.0",
    "chart.js": "^4.4.9",
    "mjml": "^4.15.3",
    "mongodb": "^6.16.0",
    "next": "^15.3.1",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "prisma-json-types-generator": "^3.3.1",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-select": "^5.10.1",
    "react-toastify": "^11.0.5",
    "sanitize-html": "^2.16.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@stryker-mutator/core": "^8.7.1",
    "@stryker-mutator/typescript-checker": "^8.7.1",
    "@stryker-mutator/vitest-runner": "^8.7.1",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/mjml": "^4",
    "@types/node": "^22.15.8",
    "@types/nodemailer": "^6",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19",
    "@types/sanitize-html": "^2.16.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-istanbul": "^3.1.3",
    "@vitest/eslint-plugin": "^1.1.36",
    "depcheck": "^1.4.7",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.21.0",
    "eslint-config-next": "^15.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-perfectionist": "^4.10.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "knip": "^5.53.0",
    "prettier": "^3.5.3",
    "prisma": "^6.7.0",
    "react-select-event": "^5.5.1",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-order": "^7.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.26.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3",
    "vitest-dom": "^0.1.1"
  },
  "engines": {
    "node": "22.14.0"
  },
  "packageManager": "yarn@4.2.2"
}
