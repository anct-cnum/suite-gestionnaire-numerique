    "@tiptap/react": "^2.11.7",
    "@tiptap/starter-kit": "^2.11.7",
    "@types/maplibre-gl": "^1.13.2",
    "chart.js": "^4.4.9",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "html2canvas": "^1.4.1",
    "maplibre-gl": "^5.5.0",
    "mjml": "^4.15.3",
    "mongodb": "^6.16.0",
    "nanoid": "5.1.5",
    "next": "^15.3.1",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "prisma-json-types-generator": "^3.2.3",
    "react": "^19.1.0",
