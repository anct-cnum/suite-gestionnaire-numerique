{
  "type": "module",
  "name": "mon-inclusion-numerique",
  "version": "0.1.0",
  "license": "AGPL-3.0",
  "scripts": {
    "bash:production": "scalingo -a mon-inclusion-numerique run --size XL bash",
    "build": "./build.sh",
    "build:dev": "next build",
    "check": "yarn dedupe --check && yarn deadcode && yarn typecheck && yarn prisma format --check && yarn format:check && yarn lint:css && yarn lint:ts && yarn test:coverage",
    "db:start": "docker compose down && docker compose up -d && bash -c \"while ! docker compose logs | grep -q 'listening on IPv4 address'; do echo 'Waiting for database'; sleep 0.5; done\";",
    "deadcode": "knip",
    "depcheck": "depcheck",
    "dev": " next dev --turbo",
    "devmigrate": "yarn prisma:migrate && yarn dev",
    "dotenv:test": "dotenv -e .env.test",
    "format": "prettier -w . \"!**/*.{js,jsx,mjs,mts,ts,tsx}\"",
    "format:check": "prettier -c . \"!**/*.{js,jsx,mjs,mts,ts,tsx}\"",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:ts": "eslint src  --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
    "migration:reset": "yarn prisma:reset",
    "postinstall": "husky || true && ./post-install.sh",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:reset": "prisma migrate reset -f ",
    "psql:local": "docker compose up -d postgres-dev && docker exec -it min_db_dev psql -U min min",
    "psql:init": "bash scripts/psql-init.sh",
    "psql:test": "docker compose up -d postgres-test && docker exec -it min_db_test psql -U min min",
    "start": "next start",
    "test": "yarn dotenv:test -- yarn prisma:migrate && yarn dotenv:test -- vitest run",
    "test:coverage": "yarn test --coverage",
    "test:mutation:frontend": "yarn dotenv:test -- stryker run stryker-frontend.conf.json",
    "test:mutation:backend": "yarn dotenv:test -- stryker run stryker-backend.conf.json",
    "test:watch": "yarn dotenv:test -- vitest",
    "typecheck": "tsc",
    "bundle:analyze": "ANALYZE=true yarn build",
    "minio:init": "bash scripts/init-minio.sh",
    "minio:start": "docker compose up -d minio && yarn minio:init",
    "update-subventions": "tsx scripts/update-subvention-status.ts",
    "update-subventions:example": "tsx scripts/update-subvention-status.ts scripts/subventions_exemple.csv",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@gouvfr/dsfr": "^1.13.1",
    "@next/bundle-analyzer": "^15.3.1",
    "@prisma/client": "^6.6.0",
    "@sentry/nextjs": "^9",
    "@tiptap/core": "^2.11.7",
